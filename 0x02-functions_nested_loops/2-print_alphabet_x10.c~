#include <stdio.h>
/**
 * main - A program to print alphabtes(lowercase) ten times.
 *
 * Return: Always 0 (Success)
 *
 */

int main(void)
{
	int ten = 0, alphabets;

	while (ten <= 10)
	{
		for (alphabets = 'a'; alphabets <= 'z')
		{
			putchar(alphabets);
		}
		ten++;
		putchar('\n');
	}
	return (0);
}
